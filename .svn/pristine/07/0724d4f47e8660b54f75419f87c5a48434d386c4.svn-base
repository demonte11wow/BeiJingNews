apply plugin: 'com.android.application'

android {
    compileSdkVersion 24
    buildToolsVersion "24.0.2"

    defaultConfig {
        applicationId "com.example.palo.beijinnews"
        minSdkVersion 17
        targetSdkVersion 24
        versionCode 1
        versionName "1.0"
    }
    buildTypes {
        release {
            minifyEnabled false
            proguardFiles getDefaultProguardFile('proguard-android.txt'), 'proguard-rules.pro'
        }
    }
}

dependencies {
    compile fileTree(include: ['*.jar'], dir: 'libs')
    testCompile 'junit:junit:4.12'
    compile 'com.android.support:appcompat-v7:24.2.1'
    compile files('libs/gson-2.4.jar')
    compile 'com.android.support:support-v4:24.2.1'
    compile project(path: ':SlidingMenu_library')
    compile project(':ViewPagerIndicator_library')
    compile files('libs/volley.jar')
    compile 'com.android.support:design:24.2.1'
    compile project(':PullToRefresh_library')
    compile files('libs/jpush-android-2.2.0.jar')
    compile files('libs/picasso-2.5.2.jar')
    compile files('libs/glide-3.7.0.jar')
    compile files('libs/uk-co-senab-photoview.jar')
    compile project(':Image_Loader_library')
    compile project(':MaterialRefreshLayout_library')
}
